def reverse_array(arr):
    return arr[::-1]
 
# Sample Input
array = [16, 18, 27, 16, 23, 21, 19]
print(f"Reverse Array elements = {reverse_array(array)}")
 
# Test cases
print(reverse_array([26, 28, 37, 26, 33, 31, 29]))
print(reverse_array([1.6, 1.8, 2.7, 1.6, 2.3, 2.1, 0.19]))
print(reverse_array([0, 160, 180, 270, 160, 230, 210, 190, 0]))
print(reverse_array([200, 180, 180, 270, 270, 270, 190, 200]))
print(reverse_array([100, 100, 100, 100, 100, 100, 100, 100]))
